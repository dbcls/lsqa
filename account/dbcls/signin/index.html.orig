    <!-- Page -->
    <!-- start page -->
<div id="page">
  <!-- start content -->
  <div id="content">

    

    <div class="section">
      <h1 class="title">
        Sign In
      </h1>
      <p class="byline" />
      <div class="entry">
        <p class="desc">
          Please input login id and password
        </p>
        <form action="/account/signin" method="post"><div style="margin:0;padding:0"><input name="authenticity_token" type="hidden" value="82c4a1f102c37d2066900e2b2d5303bcb1de5a7f" /></div>        <table class="detail">
          <tbody>
            <tr>
              <th>
                <label for="login">
                  Login ID
                </label>
              </th>
              <td>
                <input id="login" name="login" size="45" type="text" />
              </td>
            </tr>
            <tr>
              <th>
                <label for="password">
                  Password
                </label>
              </th>
              <td>
                <input class="text" id="password" name="password" size="45" type="password" value="" />
              </td>
            </tr>
          </tbody>
        </table>
        <p>
          <label for="remember_me">Keep me signed in</label>
          <input id="remember_me" name="remember_me" type="checkbox" value="1" />
        </p>
        <p><input name="commit" type="submit" value="Sign In" /></p>
        </form>      </div>
    </div>
  </div>

  <!-- start sidebars -->
